{
  "name": "vox-populi-server",
  "version": "1.0.0",
  "description": "A server to prototype a REST API over vox-populi, with an authorization layer",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "start": "nodemon index.ts",
    "prepublishOnly": "npm run compile",
