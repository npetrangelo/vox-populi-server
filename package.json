  "bugs": {
    "url": "https://github.com/npetrangelo/vox-populi-server/issues"
  },
  "homepage": "https://github.com/npetrangelo/vox-populi-server#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/node": "^7.10.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "ts-jest": "^28.0.2",
    "ts-node": "^3.2.0",
    "tslint": "^5.11.0",
    "typescript": "^4.6.4",
    "vox-populi": "^0.0.5"
